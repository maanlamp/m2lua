{
	"compilerOptions": {
		"strict": true,
		"target": "esnext",
		"module": "esnext",
		"incremental": true,
		"outDir": "./dist",
		"removeComments": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"downlevelIteration": true,
		"importHelpers": true,
		"lib": ["ESNext", "DOM"],
		"moduleResolution": "node"
	},
	"include": ["./src"]
}