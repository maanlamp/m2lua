fun fizz-buzz-if ->
	print(
		range(100)
			.map(int n ->
				`fizzbuzz` if n % 15 == 0
					else `buzz` if n % 5 == 0
						else `fizz` if n % 3 == 0
							else n)
			.join(`\n`));

fun fizz-buzz-imperative ->
	for n of 100
		var str output = ``;
		if not n % 3
			output = `fizz`;
		if not n % 5
			output = `${output}buzz`;
		print(output if not output.isEmpty else n);

for function of [fizz-buzz-if, fizz-buzz-imperative]
	function();