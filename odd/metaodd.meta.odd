program -> metarule*

metarule -> .identifier "->" chunks (.end | .EOF)

chunks -> chunk ("|" chunk)*

chunk -> ((.label? atom)+ | "(" chunks ")") quantifier?

atom -> .type
	| .identifier
	| .lexeme
	| .self

quantifier -> .quantifier | "{" .number ("," .number?)? "}"

