program -> metarule*

metarule -> .identifier "->" chunks (.end | .EOF)

chunks -> chunk+ ("|" chunk+)*

chunk -> .label? (atom | group) quantifier?

atom -> .type
	| .identifier
	| .lexeme
	| .self

group -> "(" chunks ")"

quantifier -> .quantifier | "{" .number ("," .number?)? "}"

// TODO: "{" .number ("," .number?)? "}" is somehow lexed as one lexeme??